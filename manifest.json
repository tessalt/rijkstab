{
  "name": "RijksTab",
  "description": "Override the new tab page with art",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "topSites"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
