{
  "name": "RijksTab",
  "description": "Override the new tab page with art",
  "version": "0.3",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "topSites",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
